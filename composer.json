{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.3.1",
		"silverstripe/framework": "3.3.1",
		"silverstripe/reports": "3.3.1",
		"silverstripe/siteconfig": "3.3.1",
		"unclecheese/betterbuttons": "1.2.*",
		"nglasl/silverstripe-extensible-search": "^3.0",
		"silverstripe/solr": "^5.2",
		"silverstripe/fulltextsearch-localsolr": "dev-master"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.3.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
